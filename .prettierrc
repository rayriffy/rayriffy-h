{
  "useTabs": false,
  "singleQuote": true,
  "printWidth": 80,
  "semi": false,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
